<?xml version="1.0"?>
<PropertyList>

 <path>dimmer.ac</path>

 <animation>
  <type>material</type>
  <object-name>bezel.top</object-name>
  <object-name>arrow</object-name>
  <object-name>knob</object-name>
  <emission>
    <factor-prop>controls/lighting/instruments-norm</factor-prop>
    <red>0.25</red>
    <green>0.25</green>
    <blue>0.5</blue>
  </emission>
 </animation>

  <animation>
    <type>pick</type>
    <object-name>knob</object-name>
    <visible>true</visible>
    <action>
      <button>3</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>controls/lighting/instruments-norm</property>
        <step>0.02</step>
        <min>0</min>
        <max>60</max>
        <wrap>0</wrap>
      </binding>
    </action>
 </animation>
  <animation>
    <type>pick</type>
    <object-name>knob</object-name>
    <visible>true</visible>
    <action>
      <button>4</button>
      <repeatable>true</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>controls/lighting/instruments-norm</property>
        <step>-0.02</step>
        <min>0</min>
        <max>60</max>
        <wrap>0</wrap>
      </binding>
    </action>
 </animation>

</PropertyList>


