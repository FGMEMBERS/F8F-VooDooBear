<?xml version="1.0"?>
<PropertyList>

 <path>pitchclock.ac</path>


 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>MinuteHand</object-name>
  <object-name>HourHand</object-name>
  <emission>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
    <red>0.25</red>
    <green>0.25</green>
    <blue>0.8</blue>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>HourHand</object-name>
  <property>/controls/engines/engine[0]/propeller-pitch</property>
  <factor>120.0</factor>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>MinuteHand</object-name>
  <property>/controls/engines/engine[0]/propeller-pitch</property>
  <factor>1440.0</factor>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>


 
 </PropertyList>
