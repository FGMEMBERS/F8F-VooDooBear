<?xml version="1.0"?>
<PropertyList>

 <path>P-39D-clock.ac</path>


 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>MinuteHand</object-name>
  <object-name>HourHand</object-name>
  <object-name>SecondHand</object-name>
  <emission>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
    <red>0.5</red>
    <green>0.25</green>
    <blue>0.25</blue>
  </emission>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>HourHand</object-name>
  <property>/instrumentation/clock/indicated-sec</property>
  <factor>0.00833333333333333</factor>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>MinuteHand</object-name>
  <property>/instrumentation/clock/indicated-sec</property>
  <factor>0.1</factor>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>SecondHand</object-name>
  <property>/instrumentation/clock/indicated-sec</property>
  <factor>6</factor>
  <axis>
   <x>-1</x>
    <y>0</y>
    <z>0</z>
   </axis>
  </animation>
 
 </PropertyList>
